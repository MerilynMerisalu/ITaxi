@using Base.Resources
@model WebApp.Areas.AdminArea.ViewModels.CreateEditVehicleModelViewModel

<div class="form-group">
    <label asp-for="VehicleMarkId" class="control-label"></label>
    <select asp-for="VehicleMarkId" class="form-control" asp-items="Model.VehicleMarks">
        <option>@Common.PleaseSelect</option>
    </select>
</div>
<div class="form-group">
    <label asp-for="VehicleModelName" class="control-label"></label>
    <input asp-for="VehicleModelName" class="form-control"/>
    <span asp-validation-for="VehicleModelName" class="text-danger"></span>
</div>
