<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>@ViewData["Title"] - WebApp</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css"/>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/WebApp.styles.css" asp-append-version="true"/>
</head>
<body>
<header>
    <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
        <div class="container-fluid">
            <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">WebApp</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                    aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    <li class="nav-item">
                        <a class="nav-link active" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                            Admin Area
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                            <a class="nav-link dropdown-toggle" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Counties
                            </a>
                            <li>
                                <a class="dropdown-item" asp-area="AdminArea"
                                   asp-controller="Counties"
                                   asp-action="Index">
                                    Index
                                </a>
                                <a class="dropdown-item" asp-area="AdminArea"
                                   asp-controller="Counties"
                                   asp-action="Create">
                                    Create
                                </a>
                                
                            
                            </li>
                            <li>
                                <a class="dropdown-item" asp-area="AdminArea"
                                   asp-controller="Cities" asp-action="Index">
                                    Cities
                                </a>
                            </li>
                            <li>
                                <hr class="dropdown-divider">
                            </li>
                            <li>
                                <a class="dropdown-item" asp-area="AdminArea"
                                   asp-controller="DriverLicenseCategories"
                                   asp-action="Index">
                                    Driver License Categories
                                </a>
                            </li>
                            <li>
                                <a class="dropdown-item" asp-area="AdminArea"
                                   asp-controller="Drivers"
                                   asp-action="Index">
                                    Drivers
                                </a>
                            </li>
                            <li>
                                <a class="dropdown-item" asp-area="AdminArea"
                                   asp-controller="Drives"
                                   asp-action="Index">
                                    Drives
                                </a>
                            </li>
                            <li>
                                <hr class="dropdown-divider">
                            </li>
                            <li>
                                <a class="dropdown-item" asp-area="AdminArea"
                                   asp-controller="VehicleTypes"
                                   asp-action="Index">
                                    Vehicle Types
                                </a>
                            </li>
                            <li>
                                <a class="dropdown-item" asp-area="AdminArea"
                                   asp-controller="VehicleMarks"
                                   asp-action="Index">
                                    Vehicle Marks
                                </a>
                            </li>
                            <li>
                                <a class="dropdown-item" asp-area="AdminArea"
                                   asp-controller="VehicleModels"
                                   asp-action="Index">
                                    Vehicle Models
                                </a>
                            </li>
                            <li>
                                <a class="dropdown-item" asp-area="AdminArea"
                                   asp-controller="Vehicles"
                                   asp-action="Index">
                                    Vehicles
                                </a>
                            </li>
                            <li>
                                <hr class="dropdown-divider">
                            </li>
                            <li>
                                <a class="dropdown-item" asp-area="AdminArea"
                                   asp-controller="Schedules"
                                   asp-action="Index">
                                    Schedules
                                </a>
                            </li>

                            <li>
                                <a class="dropdown-item" asp-area="AdminArea"
                                   asp-controller="RideTimes"
                                   asp-action="Index">
                                    Ride Times
                                </a>
                            </li>
                            <li>
                                <hr class="dropdown-divider">
                            </li>
                            <a class="dropdown-item" asp-area="AdminArea"
                               asp-controller="DisabilityTypes"
                               asp-action="Index">
                                Disability Types
                            </a>
                            <a class="dropdown-item" asp-area="AdminArea"
                               asp-controller="Bookings"
                               asp-action="Index">
                                Bookings
                            </a>
                            <a class="dropdown-item" asp-area="AdminArea"
                               asp-controller="Comments"
                               asp-action="Index">
                                Comments
                            </a>



                        </ul>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                            Driver Area
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                            <a class="dropdown-item" asp-area="AdminArea" 
                               asp-controller="DriverLicenseCategories"
                               asp-action="Index">
                                Driver License Categories
                            </a>
                            <li>
                                <a class="dropdown-item" href="#">Another action</a>
                            </li>
                            
                        </ul>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                            Customer Area
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                            <li>
                                <a class="dropdown-item" href="#">Action</a>
                            </li>
                            <li>
                                <a class="dropdown-item" href="#">Another action</a>
                            </li>
                            <li>
                                <hr class="dropdown-divider">
                            </li>
                            <li>
                                <a class="dropdown-item" href="#">Something else here</a>
                            </li>
                        </ul>
                    </li>

                </ul>

            </div>
            <partial name="_LoginPartial"/>
        </div>
    </nav>
</header>
<div class="container">
    <main role="main" class="pb-3">
        @RenderBody()
    </main>
</div>

<footer class="border-top footer text-muted">
    <div class="container">
        &copy; 2022 - WebApp - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
    </div>
</footer>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>
@await RenderSectionAsync("Scripts", required: false)
</body>
</html>